# Quilt MCP Server Environment Configuration
# Copy this to .env and update with your values

# AWS Configuration
AWS_PROFILE=default
CDK_DEFAULT_ACCOUNT=123456789012
CDK_DEFAULT_REGION=us-east-1
CDK_BOOTSTRAP=aws://123456789012/us-east-1
AWS_DEFAULT_REGION=us-east-1
AWS_ACCOUNT_ID=123456789012

# Quilt Configuration
QUILT_CATALOG_DOMAIN=your-catalog-domain.com

# Quilt Test Resources
QUILT_DEFAULT_BUCKET=s3://your-test-bucket
QUILT_TEST_PACKAGE=raw/test
QUILT_TEST_ENTRY=README.md
QUILT_TEST_DATABASE=userathenadatabase-mbq1ihawbzb7

# Optional log level
LOG_LEVEL=INFO

# Optional: Override FastMCP transport mode (set to STDIO for desktop clients)
FASTMCP_TRANSPORT=streamable-http

# ngrok Configuration for remote tunneling (optional)
NGROK_DOMAIN=random-free-domain.ngrok-free.app
